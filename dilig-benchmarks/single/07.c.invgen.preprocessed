int NONDET;





int main(int argc, char* argv[]) {

  int i, n, a, b;
  assume( n >= 0 );
  i = 0; a = 0; b = 0;
  while( i < n ) {
    if(NONDET) {
      a = a+1;
      b = b+2;
    } else {
      a = a+2;
      b = b+1;
    }
    i = i+1;
  }
  assert( a+b == 3*n );
}
